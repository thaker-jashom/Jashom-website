{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "routes": [
      {
        "src": "/assets/(.*)",
        "dest": "/assets/$1",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))",
        "dest": "/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }